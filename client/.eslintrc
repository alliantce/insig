{
    "extends": "airbnb",
    "rules": {
        "max-len": [2, 126, 4],
        "indent": [
            "error",
            4
        ],
        "react/jsx-indent": [
            "error",
            4
        ],
        "react/jsx-indent-props": [
            "error",
            4
        ],
        "react/prefer-stateless-function": "off",
        "import/no-extraneous-dependencies": [
            "error",
            {
                "devDependencies": [
                    "**/*.test.js?(x)"
                ]
            }
        ]
    }
}
